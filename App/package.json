{
	"name": "js-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
		"dev:wds": "webpack-dev-server --progress --mode=development",
		"prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
		"prod:start": "cross-env NODE_ENV=production pm2 start --name js-template lib/server && pm2 logs",
		"start": "NODE_ENV=production node lib/server",
		"prod:stop": "pm2 delete js-template",
		"lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
		"test": "npm run lint && flow && jest --coverage"
	},
	"author": "",
	"license": "ISC",
